## Here is an example for Python.
##
syntax "python" "\.py$"
icolor brightblue "def [0-9A-Z_]+"
color brightcyan "\<(and|as|assert|break|class|continue|def|del|elif|else|except|exec|finally|for|from|global|if|import|in|is|lambda|map|not|or|pass|print|raise|return|try|while|with)\>"
color brightyellow "\<(True|False|None|(Attribute|Index|Key|Name|OS|Overflow|Type|Value|AllInOne)Error)\>"
color brightgreen "['][^']*[^\\][']" "[']{3}.*[^\\][']{3}"  "['][']"
color brightgreen "["][^"]*[^\\]["]" "["]{3}.*[^\\]["]{3}"  "["]["]"
color brightgreen start=""""[^"]" end=""""" start="'''[^']" end="'''"
color brightred "#.*$"
